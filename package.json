{
  "name": "isa-explorer",
  "version": "1.0.0",
  "description": "\"A lightweight web-based ISA-Tab viewer for multiple ISA-Tab files\"",
  "main": "index.js",
  "scripts": {
    "test": "babel-node ./node_modules/.bin/tape 'app/test/unit/**/*.test.js'",
    "report": "NODE_ENV=test ./node_modules/.bin/nyc npm test",
    "build:local": "./node_modules/.bin/webpack --config webpack/webpack.config.local.js",
    "build:local-watch": "./node_modules/.bin/webpack --config webpack/webpack.config.local.js --watch",
    "build:prod": "./node_modules/.bin/webpack --config webpack/webpack.config.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ISA-tools/isa-explorer.git"
  },
  "keywords": [
    "ISA",
    "ISA-Tab",
    "metadata",
    "standards"
  ],
  "author": "Alejandra Gonzales-Beltran, Massimiliano Izzo",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/ISA-tools/isa-explorer/issues"
  },
  "homepage": "https://github.com/ISA-tools/isa-explorer#readme",
